<h1>Representantes</h1>
</br>
<table>
    <tr>
        <th> Nombre </th>
        <th> Apellido</th>
        <th> Edad </th>
        <th> Telefono</th>
        <th> Organizacion </th>
    </tr>
    <% @representante.each do |item| %>
    <tr>
        <td><%= item.nombre %></td>
        <td><%= item.apellido %></td>
        <td><%= item.edad %></td>
        <td><%= item.telefono %></td>
        <td><%= item.organizacion %></td>
        <td><%= link_to "Editar Post", edit_representante_path(item.id) %></td>
        <td><%= link_to "Mostrar Post", representante_path(item) %></td>
    </tr>
    <%end%>
</table>