<h1>Representantes</h1>
</br>
<table>
    <tr>
        <th> Nombre </th>
        <th> Apellido</th>
        <th> Edad </th>
        <th> Telefono</th>
        <th> Organizacion </th>
    </tr>
    <% @representante.each do |item| %>
    <tr>
        <td><%= item.nombre %></td>
        <td><%= item.apellido %></td>
        <td><%= item.edad %></td>
        <td><%= item.telefono %></td>
        <td><%= item.organizacion %></td>
        <td><%= link_to "Editar", edit_representante_path(item.id), class: "btn btn-xs btn-primary" %></td>
        <td><%= link_to "Mostrar", representante_path(item), class: "btn btn-xs btn-success" %></td>
        <td><%= link_to "Eliminar", representante_path(item), method: :delete, data: {confirm: "Seguro desea eliminar?"}, class: "btn btn-xs btn-danger" %></td>
    </tr>
    <%end%>
    <%= link_to 'AÃ±adir nuevo representante', new_representante_path, class: "btn btn-xs btn-success" %>
</table>