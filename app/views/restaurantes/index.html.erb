<h1>Restaurantes</h1>
</br>
<table>
    <tr>
        <th> Nombre </th>
        <th> Direccion</th>
        <th> Representante </th>
        <th> Telefono</th>
        <th> Pagina Web </th>
        <th> Descripcion</th>
    </tr>
    <% @restaurante.each do |item| %>
    <tr>
        <td><%= item.nombre %></td>
        <td><%= item.direccion %></td>
        <td><%= item.representante %></td>
        <td><%= item.telefonoRepresentante %></td>
        <td><%= item.direccionUrl %></td>
        <td><%= item.descripcion %></td>
        <td><%= link_to "Editar Post", edit_restaurante_path(item.id) %></td>
        <td><%= link_to "Mostrar Post", restaurante_path(item) %></td>
    </tr>
    <%end%>
    <%= link_to 'Crear nuevo post', new_post_path %>
</table>