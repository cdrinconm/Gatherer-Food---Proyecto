<h1>Llene por favor la tabla</h1>
<%= form_for @post do |f| %>
    <p>
        <%= f.label :Nombre %><br/>
        <%= f.text_field :nombre %>
    </p>
    <p>
        <%= f.label :Direccion %><br/>
        <%= f.text_field :direccion %>
    </p>
    <p>
        <%= f.label :Representante %><br/>
        <%= f.text_field :representante %>
    </p>
    <p>
        <%= f.label :Telefono_Representante %><br/>
        <%= f.text_field :telefonoRepresentante %>
    </p>
    <p>
        <%= f.label :Direccion Url %><br/>
        <%= f.text_field :direccionUrl %>
    </p>
    <p>
        <%= f.label :Descripcion %><br/>
        <%= f.text_area :descripcion %>  
    </p>
    <p>
        <%= f.submit %>
    </p>    
<% end %>
