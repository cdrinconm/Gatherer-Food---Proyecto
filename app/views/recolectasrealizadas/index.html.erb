<h1>Recolectas Realizadas</h1>
</br>
<table>
    <tr>
        <th> Organizacion </th>
        <th> Fecha</th>
        <th> Direccion </th>
        <th> Cantidad en Kilos</th>
    </tr>
    <% @recolectasrealizada.each do |item| %>
    <tr>
        <td><%= item.organizacion %></td>
        <td><%= item.fecha %></td>
        <td><%= item.direccion %></td>
        <td><%= item.cantidadKilos %></td>
        <td><%= link_to "Editar", edit_recolectasrealizada_path(item.id), class: "btn btn-xs btn-primary" %></td>
        <td><%= link_to "Mostrar", recolectasrealizada_path(item), class: "btn btn-xs btn-success" %></td>
        <td><%= link_to "Eliminar", recolectasrealizada_path(item), method: :delete, data: {confirm: "Seguro desea eliminar?"} %></td>
    </tr>
    <%end%>
    <%= link_to 'AÃ±adir nueva recolecta realizada', new_recolectasrealizada_path, class: "btn btn-xs btn-success" %>
</table>